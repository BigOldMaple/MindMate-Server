{
  "expo": {
    "name": "MindMate",
    "slug": "MindMate",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.mindmate.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.VIBRATE",
        "android.permission.HIGH_SAMPLING_RATE_SENSORS",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.ACTIVITY_RECOGNITION",
        "android.permission.health.READ_STEPS",
        "android.permission.health.READ_ACTIVITY"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 28,
            "buildToolsVersion": "34.0.0",
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResources": true,
            "useLegacyPackaging": false,
            "usesCleartextTraffic": true,
            "kotlinVersion": "1.7.20",
            "gradlePluginVersion": "8.1.0"
          }
        }
      ],
      [
        "expo-sensors",
        {
          "android": {
            "permissions": ["android.permission.HIGH_SAMPLING_RATE_SENSORS"]
          }
        }
      ],
      [
        "expo-location",
        {
          "android": {
            "permissions": [
              "android.permission.ACCESS_COARSE_LOCATION",
              "android.permission.ACCESS_FINE_LOCATION",
              "android.permission.FOREGROUND_SERVICE"
            ]
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "androidMode": "default",
          "androidCollapsedTitle": "MindMate"
        }
      ],
      [
        "expo-camera",
        {
          "android": {
            "permissions": ["android.permission.CAMERA"]
          }
        }
      ],
      [
        "expo-media-library",
        {
          "android": {
            "permissions": [
              "android.permission.READ_EXTERNAL_STORAGE",
              "android.permission.WRITE_EXTERNAL_STORAGE"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d52910f2-c495-4709-a25a-9d800ff3e91d"
      },
      "enablePerformanceLogging": false,
      "enableHermes": true
    },
    "owner": "nikodemmech",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*",
      "assets/fonts/*",
      "assets/images/*",
      "assets/sounds/*"
    ],
    "web": {
      "bundler": "metro"
    }
  }
}